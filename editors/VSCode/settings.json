{
  "beautify.language": {
    "css": ["css", "scss"],
    "html": ["htm", "html", "blade"],
    "js": {
      "filename": [".jshintrc", ".jsbeautify"],
      "type": ["javascript", "json"]
    }
  },
  "dart.allowAnalytics": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "docker-explorer.enableTelemetry": false,
  "docker-explorer.showAzureRegistries": false,
  "docker-explorer.showDockerHubTreeView": false,
  "docker-explorer.showSuggestedDockerImages": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.find.autoFindInSelection": true,
  "editor.folding": false,
  "editor.fontFamily": "'Fira Code Retina', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "off",
  "editor.minimap.renderCharacters": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.openEditors.visible": 0,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.autoSave": "onWindowChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/._*": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/_backup": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.keymap": "alternate",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "html-css-class-completion.enableEmmetSupport": true,
  "html.format.wrapAttributes": "preserve-aligned",
  "html.format.wrapLineLength": 180,
  "javascript.commonjs.resolve.extensions": [".js", ".vue"],
  "javascript.commonjs.resolve.languages": [
    "javascript",
    "javascriptreact",
    "vue",
    "vue-html"
  ],
  "npm.packageManager": "yarn",
  "prettier.disableLanguages": ["markdown"],
  "prettier.trailingComma": "es5",
  "python.jediEnabled": false,
  "python.venvPath": "$WORKON_HOME",
  "search.smartCase": true,
  "svgviewer.enableautopreview": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.linuxExec": "urxvt",
  "terminal.integrated.fontSize": 12,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "Fira Mono",
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "after": [],
      "before": ["<leader>", "<space>"],
      "commands": [
        {
          "args": [],
          "command": "workbench.action.showCommands"
